# Firebase Functions Environment Variables
# Copy this file to .env and fill in your values

# Email Configuration (vedos.cz SMTP)
EMAIL_USER=info@svatbot.cz
EMAIL_PASSWORD=your_email_password_here

# Email Settings
EMAIL_FROM=SvatBot.cz <info@svatbot.cz>

# Firebase Project
FIREBASE_PROJECT_ID=svatbot-app
FIREBASE_REGION=europe-west1

# Application URLs
APP_URL=https://svatbot.cz
ADMIN_URL=https://svatbot.cz/admin

# Stripe (for payment webhooks)
STRIPE_SECRET_KEY=sk_test_...
STRIPE_WEBHOOK_SECRET=whsec_...

